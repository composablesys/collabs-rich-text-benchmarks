    "@collabs/ws-server": "0.13.4",
    "@teamwork/websocket-json-stream": "2.0.0",
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.13",
    "@types/node": "18.15.0",
    "@types/node-os-utils": "1.3.1",
    "@types/sharedb": "3.3.2",
    "@types/ws": "8.5.2",
    "base64-js": "1.5.1",
    "body-parser": "1.20.2",
    "express": "4.18.2",
    "node-os-utils": "1.3.7",
    "protobufjs": "6.9.0",
    "quill": "1.3.7",
    "quill-cursors": "4.0.2",
    "reconnecting-websocket": "4.4.0",
    "rich-text": "4.1.0",
    "sharedb": "4.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.6.4",
    "ws": "8.13.0",
    "y-quill": "0.1.5",
    "y-websocket": "1.5.0",
    "yjs": "13.6.7"
  },
  "devDependencies": {
    "@types/quill": "1.3.10",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.2",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "html-webpack-plugin": "5.3.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "source-map-loader": "3.0.0",
    "style-loader": "3.3.2",
    "terser-webpack-plugin": "5.3.9",
    "ts-loader": "9.2.5",
    "webpack": "5.88.2",
    "webpack-cli": "4.10.0"
  },
  "scripts": {
    "start": "TS_NODE_PROJECT='tsconfig-server.json' ts-node src/server/static.ts",
    "yjs-server": "PORT=8081 y-websocket",
    "collabs-server": "PORT=8081 collabs-ws-server",
    "build:protobuf": "npm-run-all build:protobuf:*",
    "build:protobuf:prepare": "mkdir -p generated",
    "build:protobuf:js": "pbjs -t static-module -w es6 -o generated/proto_compiled.js `find src -name '*.proto'`",
    "build:protobuf:ts": "pbts -o generated/proto_compiled.d.ts generated/proto_compiled.js",
    "dev": "npm run build:protobuf && npm run dev:webpack",
    "dev:webpack": "TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack",
    "build": "npm run build:protobuf && npm run build:webpack",
    "build:webpack": "TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack --mode production --devtool source-map",
    "aws": "TS_NODE_PROJECT='tsconfig-aws.json' ts-node src/aws/main.ts",
    "clean": "rm -rf build generated"
  },
  "engines": {
    "node": "^18.0.0"
  }
}
